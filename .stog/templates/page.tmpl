<script type="text/javascript">
function showtab(boxid,contentid,tabsid,tabid) {
  var x = document.querySelectorAll('#'+boxid+' div.tab-content');
  for (let i of x) {
    console.log(i);
    i.classList.add('nodisplay') ;
  }
  document.getElementById(contentid).classList.remove('nodisplay');

  var y = document.querySelectorAll('#'+tabsid+' li');
  for (let i of y) {
    console.log(i);
    i.classList.remove('selected') ;
  }
  document.getElementById(tabid).classList.add('selected');
}
</script>
